 <h3>Dragon</h3>
 <h3>Filter Dragons By House</h3>
<form method="GET" action="">
    <input type="text" name="House" value="">
    <br>
    <input type="submit" value="Submit">
</form>

<table align="center">
    <tr>
        <th>Dragon Name</th>
        <th>Rider</th>
        <th>Identification</th>
        <th>House</th>
    </tr>
    {{#each dragons as |dragon|}}
    <tr>
        <th>{{dragon.dragonName}}</th>
        <th>{{dragon.rider}}</th>
        <th>{{dragon.identification}}</th>
        <th>{{dragon.house}}</th>
    </tr>
  {{/each}}
</table>